{
  "providerUuid": "8aa7746c-106d-485f-9b47-0a1007448a3f",
  "blueprintUuid": "{{ blueprintUuid }}",
  "blueprint": {
    "name": "{{ tower_template }}",
    "api_version": "v{{ version }}",
    "rules": {
             "groupRestriction": "ONLY_SANDBOX"
        },
    "description": "{{ description }}",
    "nameTemplate": "{{ tower_template }}",
    "pages": {
      "list": {
        "key": "main",
        "name": "Main"
      }
    },
    "fields": {
      "list": [
        {%- for field in arguments %}
        {
           {% include 'fields.j2' %}
        }
        {%- if not loop.last  %},  {%-  endif  %}
        {%-  endfor  %}
      ]
    },
   "requestTypes": {
      "hermes": {
        "createSpace": {
          "events": {
            "submit": {
              "callback": {
                "enabled": true,
                "template": "devops-marketplace.yaml",
                "config": {
                  "snack": "{{ tower_template }}"
                }
              }
            }
          }
        }
      }
    }
   },
  "image": ""
}
